{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  },
  "routes": [
    {
      "route": "/data/*.json",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ]
}
